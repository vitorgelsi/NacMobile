<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/americanas"
        android:layout_width="179dp"
        android:layout_height="160dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/ebay"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/americanas" />

    <ImageView
        android:id="@+id/buscape"
        android:layout_width="139dp"
        android:layout_height="107dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="41dp"
        app:layout_constraintBottom_toTopOf="@+id/mercado"
        app:layout_constraintStart_toEndOf="@+id/americanas"
        app:srcCompat="@drawable/buscape" />

    <ImageView
        android:id="@+id/ebay"
        android:layout_width="154dp"
        android:layout_height="87dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/magalu"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ebay" />

    <ImageView
        android:id="@+id/magalu"
        android:layout_width="142dp"
        android:layout_height="85dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/submarino"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/magalu" />

    <ImageView
        android:id="@+id/netshoes"
        android:layout_width="173dp"
        android:layout_height="120dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="17dp"
        app:layout_constraintBottom_toTopOf="@+id/webmotors"
        app:layout_constraintStart_toEndOf="@+id/magalu"
        app:srcCompat="@drawable/netshoes" />

    <ImageView
        android:id="@+id/submarino"
        android:layout_width="146dp"
        android:layout_height="111dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="124dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/submarino" />

    <ImageView
        android:id="@+id/mercado"
        android:layout_width="195dp"
        android:layout_height="108dp"
        android:layout_marginStart="18dp"
        android:layout_marginBottom="9dp"
        app:layout_constraintBottom_toTopOf="@+id/netshoes"
        app:layout_constraintStart_toEndOf="@+id/ebay"
        app:srcCompat="@drawable/mercado" />

    <ImageView
        android:id="@+id/webmotors"
        android:layout_width="180dp"
        android:layout_height="149dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="124dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/submarino"
        app:srcCompat="@drawable/webmotors" />
</androidx.constraintlayout.widget.ConstraintLayout>